<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @curso_pref_regional.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @curso_pref_regional.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="curso_pref_regional[id_estabelecimento]" class="fieldLabel">Id estabelecimento</label>
            <input type="text" id="curso_pref_regional[id_estabelecimento]" name="curso_pref_regional[id_estabelecimento]" <%= placeholder("Id estabelecimento") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="curso_pref_regional[id_curso]" class="fieldLabel">Id curso</label>
            <input type="text" id="curso_pref_regional[id_curso]" name="curso_pref_regional[id_curso]" <%= placeholder("Id curso") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="curso_pref_regional[id_distrito]" class="fieldLabel">Id distrito</label>
            <input type="text" id="curso_pref_regional[id_distrito]" name="curso_pref_regional[id_distrito]" <%= placeholder("Id distrito") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="curso_pref_regional[percentagem]" class="fieldLabel">Percentagem</label>
            <input type="text" id="curso_pref_regional[percentagem]" name="curso_pref_regional[percentagem]" <%= placeholder("Percentagem") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
