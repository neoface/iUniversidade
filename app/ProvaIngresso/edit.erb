<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @prova_ingresso.id_prova_ing %></h1>
    <a href="<%= url_for :action => :show, :id => @prova_ingresso.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @prova_ingresso.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @prova_ingresso.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="prova_ingresso[id_prova_ing]" class="fieldLabel">Id prova ing</label>
            <input type="text" id="prova_ingresso[id_prova_ing]" name="prova_ingresso[id_prova_ing]" value="<%= @prova_ingresso.id_prova_ing %>" <%= placeholder( "Id prova ing" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="prova_ingresso[prova_ingresso]" class="fieldLabel">Prova ingresso</label>
            <input type="text" id="prova_ingresso[prova_ingresso]" name="prova_ingresso[prova_ingresso]" value="<%= @prova_ingresso.prova_ingresso %>" <%= placeholder( "Prova ingresso" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="prova_ingresso[descricao]" class="fieldLabel">Descricao</label>
            <input type="text" id="prova_ingresso[descricao]" name="prova_ingresso[descricao]" value="<%= @prova_ingresso.descricao %>" <%= placeholder( "Descricao" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
