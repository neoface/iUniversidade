<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @prova_ingresso.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @prova_ingresso.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="prova_ingresso[id_prova_ing]" class="fieldLabel">Id prova ing</label>
            <input type="text" id="prova_ingresso[id_prova_ing]" name="prova_ingresso[id_prova_ing]" <%= placeholder("Id prova ing") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="prova_ingresso[prova_ingresso]" class="fieldLabel">Prova ingresso</label>
            <input type="text" id="prova_ingresso[prova_ingresso]" name="prova_ingresso[prova_ingresso]" <%= placeholder("Prova ingresso") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="prova_ingresso[descricao]" class="fieldLabel">Descricao</label>
            <input type="text" id="prova_ingresso[descricao]" name="prova_ingresso[descricao]" <%= placeholder("Descricao") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
