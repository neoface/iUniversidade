<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New ProvaIngresso</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @prova_ingresso.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id prova ing: </td>
                <td class="itemValue"><input type="text" name="prova_ingresso[id_prova_ing]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Prova ingresso: </td>
                <td class="itemValue"><input type="text" name="prova_ingresso[prova_ingresso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Descricao: </td>
                <td class="itemValue"><input type="text" name="prova_ingresso[descricao]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
