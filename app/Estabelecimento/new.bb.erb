<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Estabelecimento</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @estabelecimento.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id estabelecimento: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[id_estabelecimento]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Estabelecimento: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[estabelecimento]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Id distrito: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[id_distrito]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Tipo ensino: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[tipo_ensino]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Morada: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[morada]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Localidade: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[localidade]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Cod postal: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[cod_postal]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Telefone: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[telefone]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Fax: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[fax]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Email: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[email]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Site: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[site]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Mapa: </td>
                <td class="itemValue"><input type="text" name="estabelecimento[mapa]"/></td>
            </tr>
          
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
