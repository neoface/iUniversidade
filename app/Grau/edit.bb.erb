<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @grau.object %>
    <h1>Edit <%= @grau.id_grau%></h1>
    <%= link_to "Delete", :action => :delete, :id => @grau.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @grau.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id grau: </td>
              <td class="itemValue"><input type="text" name="grau[id_grau]" value="<%= @grau.id_grau %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Grau: </td>
              <td class="itemValue"><input type="text" name="grau[grau]" value="<%= @grau.grau %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
