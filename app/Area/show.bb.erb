<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @area.id_area %></h1>
    <%= link_to "Edit", :action => :edit, :id => @area.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">IdArea</td>
        <td class="itemValue"><%= @area.id_area%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Area</td>
        <td class="itemValue"><%= @area.area%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Descricao</td>
        <td class="itemValue"><%= @area.descricao%></td>
      </tr>
      
    </table>
  </div>

</div>
