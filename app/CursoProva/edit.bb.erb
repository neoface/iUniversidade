<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @curso_prova.object %>
    <h1>Edit <%= @curso_prova.id_estabelecimento%></h1>
    <%= link_to "Delete", :action => :delete, :id => @curso_prova.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @curso_prova.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id estabelecimento: </td>
              <td class="itemValue"><input type="text" name="curso_prova[id_estabelecimento]" value="<%= @curso_prova.id_estabelecimento %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Id curso: </td>
              <td class="itemValue"><input type="text" name="curso_prova[id_curso]" value="<%= @curso_prova.id_curso %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Grupo prova id prova ing: </td>
              <td class="itemValue"><input type="text" name="curso_prova[grupo_prova_id_prova_ing]" value="<%= @curso_prova.grupo_prova_id_prova_ing %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
