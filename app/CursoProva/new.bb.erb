<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New CursoProva</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @curso_prova.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id estabelecimento: </td>
                <td class="itemValue"><input type="text" name="curso_prova[id_estabelecimento]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Id curso: </td>
                <td class="itemValue"><input type="text" name="curso_prova[id_curso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Grupo prova id prova ing: </td>
                <td class="itemValue"><input type="text" name="curso_prova[grupo_prova_id_prova_ing]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
