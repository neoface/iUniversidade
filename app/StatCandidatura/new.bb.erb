<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New StatCandidatura</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @stat_candidatura.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id estabelecimento: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[id_estabelecimento]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Id curso: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[id_curso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Ano lectivo: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[ano_lectivo]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Fase: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[fase]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Vagas: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[vagas]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Candidatos m: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[candidatos_m]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Candidatos f: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[candidatos_f]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Candidatos 1 opcao: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[candidatos_1_opcao]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Colocados m: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[colocados_m]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Colocados f: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[colocados_f]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Colocados 1 opcao: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[colocados_1_opcao]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Media nota cand: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[media_nota_cand]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Media prova ingresso: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[media_prova_ingresso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Media nota 12: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[media_nota_12]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Media nota 11: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[media_nota_11]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Nota ultimo colocado: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[nota_ultimo_colocado]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Novo reformulado flag: </td>
                <td class="itemValue"><input type="text" name="stat_candidatura[novo_reformulado_flag]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
