<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @v_curso.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @v_curso.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="v_curso[id_estabelecimento]" class="fieldLabel">Id estabelecimento</label>
            <input type="text" id="v_curso[id_estabelecimento]" name="v_curso[id_estabelecimento]" <%= placeholder("Id estabelecimento") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="v_curso[id_distrito]" class="fieldLabel">Id distrito</label>
            <input type="text" id="v_curso[id_distrito]" name="v_curso[id_distrito]" <%= placeholder("Id distrito") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="v_curso[curso]" class="fieldLabel">Curso</label>
            <input type="text" id="v_curso[curso]" name="v_curso[curso]" <%= placeholder("Curso") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="v_curso[grau]" class="fieldLabel">Grau</label>
            <input type="text" id="v_curso[grau]" name="v_curso[grau]" <%= placeholder("Grau") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="v_curso[estabelecimento]" class="fieldLabel">Estabelecimento</label>
            <input type="text" id="v_curso[estabelecimento]" name="v_curso[estabelecimento]" <%= placeholder("Estabelecimento") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
