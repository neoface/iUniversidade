<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New VCurso</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @v_curso.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id estabelecimento: </td>
                <td class="itemValue"><input type="text" name="v_curso[id_estabelecimento]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Id distrito: </td>
                <td class="itemValue"><input type="text" name="v_curso[id_distrito]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Curso: </td>
                <td class="itemValue"><input type="text" name="v_curso[curso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Grau: </td>
                <td class="itemValue"><input type="text" name="v_curso[grau]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Estabelecimento: </td>
                <td class="itemValue"><input type="text" name="v_curso[estabelecimento]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
